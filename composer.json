{
    "name": "minormous/framework",
    "description": "A tiny framework that can start anything.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Minormous Contributors",
            "homepage": "https://github.com/minormous"
        }
    ],
    "require": {
        "php": ">=8.3",
        "minormous/config": "dev-master",
        "minormous/structure": "dev-master",
        "filp/whoops": "^2.0",
        "lukasoppermann/http-status": "^2",
        "nikic/fast-route": "^1.0",
        "psr/log": "^1.0",
        "rdlowrey/auryn": "^1.4",
        "relay/relay": "^1.1",
        "relay/middleware": "^1.0",
        "willdurand/negotiation": "^3.1",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "josegonzalez/dotenv": "^2.0",
        "league/plates": "^3.1",
        "monolog/monolog": "^1.19",
        "laminas/laminas-diactoros": "^1.0.4"
    },
    "suggest": {
        "josegonzalez/dotenv": "For environment based configuration loading",
        "league/plates": "For simple HTML templates written in PHP",
        "monolog/monolog": "For a simple logging implementation of PSR-3"
    },
    "autoload": {
        "psr-4": {
            "Minormous\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MinormousTests\\": "tests/"
        }
    }
}
